<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.maps.MapsFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="-16dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#00F31111">

            <ImageButton
                android:id="@+id/Ascentbtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginStart="131dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="132dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/ascent_maps"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Fracturebtn" />

            <ImageButton
                android:id="@+id/Bindbtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="32dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/bind_maps"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Havenbtn" />

            <ImageButton
                android:id="@+id/Breezebtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="32dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/breeze_maps"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/IceBoxbtn" />

            <ImageButton
                android:id="@+id/Fracturebtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="32dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/fracture_maps"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Bindbtn" />

            <ImageButton
                android:id="@+id/IceBoxbtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="32dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/icebox_maps"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Splitbtn" />

            <ImageButton
                android:id="@+id/Splitbtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="32dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/split_maps"
                android:tint="#00FFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/MapsTitle" />

            <ImageButton
                android:id="@+id/Havenbtn"
                android:layout_width="170dp"
                android:layout_height="141dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="32dp"
                android:backgroundTint="#00FFFFFF"
                android:scaleType="fitXY"
                android:src="@drawable/haven_maps"
                android:tint="#00FFFFFF"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/MapsTitle" />

            <ImageView
                android:id="@+id/MapsTitle"
                android:layout_width="110dp"
                android:layout_height="50dp"
                android:layout_marginStart="130dp"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="131dp"
                android:src="@drawable/maps"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>